{
    "family": "k-task-management-task",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "k-task-management-container",
            "image": "public.ecr.aws/nginx/nginx:latest",
            "portMappings": [
                {
                    "containerPort": 3000,
                    "hostPort": 3000,
                    "protocol": "tcp"
                }
            ],
            "essential": true,
            "environment": [
                {
                    "name": "PORT",
                    "value": "3000"
                },
                {
                    "name": "DB_PORT",
                    "value": "5432"
                },
                {
                    "name": "DB_DATABASE",
                    "value": "task_management"
                },
                {
                    "name": "DB_USERNAME",
                    "value": "postgres"
                },
                {
                    "name": "DB_HOST",
                    "value": "k-task-management-alb-1735786431.us-east-1.elb.amazonaws.com"
                }
            ],
            "secrets": null,
            "mountPoints": [],
            "volumesFrom": [],
            "logConfiguration": {
                "logDriver": "awslogs",
                "options": {
                    "awslogs-group": "/ecs/k-task-management-task",
                    "awslogs-region": "us-east-1",
                    "awslogs-stream-prefix": "ecs"
                }
            }
        }
    ],
    "requiresCompatibilities": [
        "FARGATE"
    ],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::298108674164:role/k-task-management-ecs-task-execution-role"
}